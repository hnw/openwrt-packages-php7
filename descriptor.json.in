{
    "package": {
        "subject": "hnw",
        "repo": "openwrt-packages",
        "name": "php7",
        "desc": "PHP7 Hypertext preprocessor",
        "website_url": "http://www.php.net/",
        "issue_tracker_url": "https://github.com/hnw/openwrt-packages-php7/issues",
        "vcs_url": "https://github.com/hnw/openwrt-packages-php7.git",
        "licenses": ["PHP-3.0"],
        "public_download_numbers": false,
        "public_stats": false
    },
    "version": {
        "name": "${VERSION}",
        "desc": "${DESC}",
	"vcs_tag": "${VCS_TAG}",
        "released": "${RELEASED}",
        "github_use_tag_release_notes": false,
        "gpgSign": false
    },
    "files": [
	{
	    "includePattern": "${TRAVIS_BUILD_DIR}/pkgs-for-bintray/(.*\\.ipk)",
	    "uploadPattern": "${DISTRO}/${ARCH}${UPLOAD_DIR_SUFFIX}/$1",
	    "matrixParams": { "override": 1 }
	}
    ],
    "publish": true
}
